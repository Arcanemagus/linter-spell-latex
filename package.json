{
  "name": "linter-spell-latex",
  "main": "./lib/main",
  "version": "0.3.0",
  "description": "Provider for Atom linter-spell package to enable LaTeX spell checking.",
  "author": "Tarn Burton <twburton@gmail.com>",
  "keywords": [
    "tex",
    "latex",
    "lint",
    "linter",
    "spellcheck",
    "spelling"
  ],
  "repository": "https://github.com/yitzchak/linter-spell-latex",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0"
  },
  "bugs": {
    "url": "https://github.com/yitzchak/linter-spell-latex/issues"
  },
  "dependencies": {
    "lodash": "^4.13.0",
    "atom-package-deps": "^4.0.0"
  },
  "package-deps": [
    "linter-spell"
  ],
  "configSchema": {
    "ignoredArgumentCommands": {
      "title": "Ignored Argument Commands",
      "description": "Commands that have a single required argument and one optional argument in which the arguments are ignored.",
      "type": "array",
      "items": {
        "type": "string"
      },
      "default": [
        "addtocounter",
        "begin",
        "bibitem",
        "bibliography",
        "bibliographystyle",
        "cite",
        "documentclass",
        "end",
        "ensuremath",
        "label",
        "newenvironment",
        "newtheorem",
        "pageref",
        "ref",
        "refstepcounter",
        "renewenvironment",
        "setcounter",
        "stepcounter",
        "thispagestyle",
        "usecounter",
        "usepackage",
        "value",
        "vspace"
      ],
      "order": 1
    },
    "ignoredEnvironments": {
      "title": "Ignored Environments",
      "description": "Environments in which to ignore the contents.",
      "type": "array",
      "items": {
        "type": "string"
      },
      "default": [
        "displaymath",
        "equation",
        "equation*",
        "filecontents"
      ],
      "order": 1
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "atom",
      "latex",
      "afterEach",
      "beforeEach",
      "describe",
      "expect",
      "it",
      "jasmine",
      "runs",
      "spyOn",
      "waitsFor",
      "waitsForPromise"
    ]
  },
  "providedServices": {
    "linter-spell-grammar": {
      "versions": {
        "1.0.0": "provideGrammar"
      }
    }
  }
}
